{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@Navigation/*": ["src/navigation/*"],
      "@Components/*": ["src/components/*"],
      "@PhoneApplications/*": ["src/components/phoneApplications"],
      "@Web/*": ["src/web/*"],
      "@Assets/*": ["assets/*"]
    }
  },
  "types": [
    "node",
    "webpack-env" // here
  ],
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
  "exclude": ["node_modules"]
}
