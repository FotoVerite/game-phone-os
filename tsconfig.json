{
  "compilerOptions": {
    "module": "ES2020",
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "types": [
    "node",
    "webpack-env" // here
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "eslintrc.js"
  ],
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
